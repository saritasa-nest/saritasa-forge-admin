@inherits CustomField

<MudSelect @bind-Value="PropertyValue"
           ToStringFunc="navigation => ConvertNavigationToString(navigation)"
           Label="@Property.Name"
           Variant="Variant.Outlined"
           HelperText="@Property.Description"
           Disabled="IsReadOnly"
           Clearable="Property.IsNullable"
           Error="@FieldErrorModel.IsError"
           ErrorText="@FieldErrorModel.ErrorMessage">
    @foreach (var instance in NavigationInstances)
    {
        <MudSelectItem Value="instance" />
    }
</MudSelect>
